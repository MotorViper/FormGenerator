    </Style>
    <Style TargetType="{x:Type Label}" x:Key="SideHeading" >
      <Setter Property="FontWeight" Value="Bold"/>
      <Setter Property="FontSize" Value="22"/>
      <Setter Property="FontFamily" Value="Blackadder ITC"/>
      <Setter Property="HorizontalAlignment" Value="Center"/>
      <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="CentreLabel" BasedOn="{StaticResource Stretch}" >
      <Setter Property="HorizontalContentAlignment" Value="Center"/>
      <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="Inverted" BasedOn="{StaticResource CentreLabel}" >
      <Setter Property="Background" Value="Black"/>
      <Setter Property="Foreground" Value="White"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="PlusLabel" BasedOn="{StaticResource CentreLabel}" >
      <Setter Property="Content" Value="+"/>
      <Setter Property="Width" Value="16"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="BorderedLabel" BasedOn="{StaticResource CentreLabel}" >
      <Setter Property="BorderThickness" Value="1"/>
      <Setter Property="BorderBrush" Value="Black"/>
      <Setter Property="Margin" Value="1"/>
      <Setter Property="Height" Value="26"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="LabelUnder" >
      <Setter Property="Margin" Value="0,-6,0,0"/>
      <Setter Property="FontSize" Value="8"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="LabelUnderLeft" BasedOn="{StaticResource LabelUnder}" >
      <Setter Property="HorizontalAlignment" Value="Left"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="WeaponHeader" BasedOn="{StaticResource CentreLabel}" >
      <Setter Property="BorderThickness" Value="0"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="SkillHeader" BasedOn="{StaticResource CentreLabel}" >
      <Setter Property="BorderThickness" Value="1"/>
      <Setter Property="BorderBrush" Value="Black"/>
    </Style>
    <Style TargetType="{x:Type Label}" x:Key="SkillEntry" >
