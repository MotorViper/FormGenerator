	Eyes:
	Hair:
	Skin:
	DexModifier: ($Dexterity - 10) / 2
	StrModifier: ($Strength - 10) / 2
	ConModifier: ($Constitution - 10) / 2
	WisModifier: ($Wisdom - 10) / 2
	IntModifier: ($Intelligence - 10) / 2
	ChaModifier: ($Charisma - 10) / 2
	ClassList: AGG:{Level.Class}
	HP: SUM:$Level.HP
	ArmourBonus: {Armour.{Armour}.ACBonus}
	ShieldBonus: 0
	FlatFooted: 10 + $ArmourBonus + $SizeACModifier
	AC: $FlatFooted + $DexModifier
	Touch: 10 + $DexModifier + $SizeACModifier
	MiscInitiative: 0
	Initiative: $DexModifier + $MiscInitiative
	SizeACModifier: {Size.{Races.{Race}.Size}.ACModifier}
	NaturalArmour: {Races.{Race}.ACBonus}
	DeflectionModifier: 0
	MiscACModifier: 0
	CalculateBAB: INT:CASE:($P1|Poor|$P2 / 2|Average|$P2 * 3 / 4|Good|$P2)
	BABThrow: SUM:OVER:({Classes.ALL.NAME}|cl|CalculateBAB:({Classes.{cl}.{P1}}|COUNT:{Level.Class={cl}}))
	CalculateSave: INT:COMP:($P1|Poor|$P2/3|2 + $P2/2)
	SavingThrow: SUM:OVER:({Classes.ALL.NAME}|cl|CalculateSave:({Classes.{cl}.{P1}}|COUNT:{Level.Class={cl}}))
	FortitudeBase: SavingThrow: Fort
	ReflexBase: SavingThrow: Ref
	WillBase: SavingThrow: Will
	FortitudeThrow: $ConModifier + $FortitudeBase
	ReflexThrow: $DexModifier + $ReflexBase
	WillThrow: $WisModifier + $WillBase
	BAB: BABThrow: BABonus
	SpellResistance: 0
	SizeBABModifier: 0
	MiscBABModifier: 0
	Grapple: $BAB + $StrModifier + $SizeBABModifier + $MiscBABModifier
	Melee: $BAB + $StrModifier
	Missile: $BAB + $DexModifier
	IsClassSkill: CONTAINS:({Classes.({Level.Class}#0).ClassSkills}|$TABLEITEM)
	SkillPoints: SUM:{Level.Skills.{TABLEITEM}}
	SkillModifier: {{Skills.{TABLEITEM}.Key}Modifier}
	CanUseSkill : OR:(IF:({Skills.{TABLEITEM}.TrainedOnly}|False|True)|COMP:(0|$SkillPoints|False|False|True))

